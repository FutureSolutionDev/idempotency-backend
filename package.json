{
  "name": "idempotency",
  "version": "1.0.0",
  "description": "Backend idempotency middleware using key+id to support frontend multi-request IDs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/src",
    "dist/index.js",
    "dist/index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "idempotency",
    "express",
    "middleware",
    "nodejs",
    "fastify"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest --config jest.config.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/supertest": "^6.0.3",
    "express": "^4.18.2",
    "fastify": "^5.4.0",
    "jest": "^30.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}